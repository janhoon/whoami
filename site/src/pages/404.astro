    <h2 class="text-2xl font-bold">Not Found</h2>
    <p class="text-gray-400">Could not find requested resource</p>
    <a
      href="/"
      class="flex items-center gap-2 pt-4 text-green-400 hover:text-green-500"
    >
      Return Home
    </a>
  </div>
</BaseLayout>
